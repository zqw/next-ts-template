{
    "extends": [
        "standard",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "parser":"babel-eslint",
    "plugins": [
        "react"
    ],
    "rules": {
        "no-return-assign": "warn"
    }
}